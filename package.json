{
  "name": "@gaincompliance/eslint-config-gain",
  "license": "MIT",
  "description": "shared eslint rules for gain projects",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "test": "eslint rules",
    "preversion": "npm test",
    "precommit": "npm test",
    "commitmsg": "commitlint -e"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GainCompliance/eslint-config-gain.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "eslint",
    "react",
    "jsx",
    "es6",
    "mocha",
    "cucumber"
  ],
  "author": "Gain Compliance",
  "bugs": {
    "url": "https://github.com/GainCompliance/eslint-config-gain/issues"
  },
  "homepage": "https://github.com/GainCompliance/eslint-config-gain#readme",
  "dependencies": {
    "eslint": "7.17.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-cucumber": "1.4.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-mocha": "8.0.0",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0"
  },
  "devDependencies": {
    "commitlint-config-gain": "1.0.27",
    "cz-conventional-changelog": "3.3.0",
    "husky": "4.3.6",
    "validate-commit-msg": "2.14.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
